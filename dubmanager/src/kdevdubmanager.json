{
    "KPlugin": {
        "Category": "Project Management",
        "Description": "DUB Project Manager",
        "Id": "KDevDUBManager",
        "Name": "DUB Project Manager",
        "Authors": [
            { "Name": "Jaap Geurts",
            "Email": "jaap.geurts@fontys.nl"
            }],
        "Version": "0.1",
        "License": "GPL",
        "Icon": "dub",
        "ServiceTypes": [
            "KDevelop/Plugin"
        ]
    },
    "X-KDevelop-Category": "Project",
    "X-KDevelop-Interfaces": [
        "org.kdevelop.IBuildSystemManager",
        "org.kdevelop.IProjectFileManager"
    ],
    "X-KDevelop-IRequired": [
        "org.kdevelop.IOutputView"
    ],
    "X-KDevelop-Mode": "GUI",
    "X-KDevelop-FileManager": "D",
    "X-KDevelop-Languages": [ "D" ],
    "X-KDevelop-ProjectFilesFilter": [
        "*.json",
        "*.sdl"
    ],
    "X-KDevelop-ProjectFilesFilterDescription": "DUB Project Files",
    "X-KDevelop-SupportedMimeTypes": [
        "text/x-dsrc",
        "application/json"
    ]
}
